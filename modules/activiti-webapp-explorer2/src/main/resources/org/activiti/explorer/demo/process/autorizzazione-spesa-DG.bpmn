      <extensionElements>
        <activiti:formProperty id="voceprogetto" name="Voce di Costo" type="projectCostItem" variable="voceprogetto" required="true"></activiti:formProperty>
        <activiti:formProperty id="oggettoprogetto" name="Oggetto/Motivazione" type="string" variable="oggettoprogetto" required="true"></activiti:formProperty>
        <activiti:formProperty id="importo" name="Spesa/Impegno" type="long" variable="importo" required="true"></activiti:formProperty>
        <activiti:formProperty id="visto" name="VISTO" type="textarea" variable="visto"></activiti:formProperty>
        <activiti:formProperty id="applicazioneva" name="Applica l'IVA" type="boolean" variable="applicazioneiva"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="sid-49F646B0-DC0D-42E7-A823-80DC461519E3" name="Autorizzazione SPF" activiti:exclusive="false" activiti:assignee="emulas">
      <extensionElements>
        <activiti:formProperty id="nome-progetto-spf" name="Voce di Costo" type="string" variable="voceprogetto.idProject" default="${voceprogetto.idProject}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="oggetto-progetto-spf" name="Oggetto Progetto" variable="oggettoprogetto" writable="false"></activiti:formProperty>
        <activiti:formProperty id="importoread" name="Importo" type="long" expression="#{importo}" variable="importo" writable="false"></activiti:formProperty>
        <activiti:formProperty id="applicaiva" name="Applicazione IVA?" type="boolean" variable="applicazioneiva"></activiti:formProperty>
        <activiti:formProperty id="autorizzazionespesa" name="Autorizza?" type="enum" variable="autorizzazionespesa" required="true">
          <activiti:value id="true" name="Si"></activiti:value>
          <activiti:value id="false" name="No"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
