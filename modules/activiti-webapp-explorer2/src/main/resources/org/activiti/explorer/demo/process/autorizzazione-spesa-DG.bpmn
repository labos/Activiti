    <serviceTask id="sid-E8BF3436-8835-47DC-A5F3-12D2B2BC890B" name="Notifica email di Rifiuto al proponente" activiti:exclusive="false" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${emailInitiator}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:expression>responsabile@sardegnaricerche.it</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression>Rifiuto autorizzazione spf ${motivazioneriufiutoautorizzazione}</activiti:expression>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression>Rifiuto: ${motivazioneriufiutoautorizzazione}</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>E' stato rifiutato da SPF</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
