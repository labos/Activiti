  <process id="determinazione-DG-noProposta" name="Determinazione DG senza proposta" isExecutable="true" activiti:candidateStarterGroups="user">
    <startEvent id="startevent1" name="Start" activiti:initiator="initiator"></startEvent>
    <userTask id="usertask1" name="Nuova proposta di determinazione ${detSubject}" activiti:assignee="${detAssignee}">
      <documentation>A CAUSA DEL RIFIUTO DELLA PROPOSTA PRECEDENTE, DEVI ALLEGARNE UNA NUOVA.</documentation>
    </userTask>
    <userTask id="usertask2" name="Approvazione DG determinazione di ${detSubject}" activiti:assignee="aatzeni">
      <extensionElements>
        <activiti:formProperty id="firmatoDG" name="Firma DG" type="enum" variable="firmatoDG" required="true">
          <activiti:value id="true" name="Sì"></activiti:value>
          <activiti:value id="false" name="No"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="motivazioneDG" name="Motivazione" type="string" variable="motivazioneDG"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <userTask id="usertask3" name="AGI - Assegnazione numero e data" activiti:assignee="smaxia">
      <documentation>Assegnare numero e data alla determinazione di ${detSubject}.</documentation>
      <extensionElements>
        <activiti:formProperty id="data_determinazione" name="Data determinazione" type="date" variable="data_determinazione" required="true"></activiti:formProperty>
        <activiti:formProperty id="numero_determinazione" name="Numero determinazione" type="long" variable="numero_determinazione" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <serviceTask id="servicetask1" name="Check proposta" activiti:class="org.sr.activiti.explorer.delegates.AttachmentCheck">
      <extensionElements>
        <activiti:field name="minAttachmentsNum">
          <activiti:string>1</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="servicetask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow4" name="Non allegato" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isAttached}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="Allegato" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isAttached}]]></conditionExpression>
    </sequenceFlow>
    <parallelGateway id="parallelgateway1" name="Parallel Gateway"></parallelGateway>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow15" name="Archiviato" sourceRef="exclusivegateway4" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isArchived}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow16" name="Non archiviato" sourceRef="exclusivegateway4" targetRef="parallelgateway1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isArchived}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="Problema archiviazione" activiti:assignee="smaxia">
      <documentation>ATTENZIONE: SI È VERIFICATO UN PROBLEMA NELL'ARCHIVIAZIONE DEI FILE NEL DOCUMENTALE. CONTATTARE I SISTEMISTI E COMPLETARE IL TASK APPENA RISOLTO IL PROBLEMA.</documentation>
    </userTask>
    <serviceTask id="mailtask1" name="Notifica problema archiviazione" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${emailInitiator}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>responsabile@sardegnaricerche.it</activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Problema archiviazione documentale</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:string>Si è verificato un problema nell'archiviazione dei documenti. Contattare i sistemisti e completare il task una volto risolto il problema.</activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:string>Si è verificato un problema nell'archiviazione dei documenti. Contattare i sistemisti e completare il task una volto risolto il problema.</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow17" sourceRef="parallelgateway1" targetRef="mailtask1"></sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="parallelgateway1" targetRef="usertask5"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway5" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow20" sourceRef="usertask2" targetRef="exclusivegateway5"></sequenceFlow>
    <parallelGateway id="parallelgateway2" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow21" name="Non approvato" sourceRef="exclusivegateway5" targetRef="parallelgateway2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${firmatoDG=='false'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow22" sourceRef="parallelgateway2" targetRef="mailtask2"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="parallelgateway2" targetRef="usertask1"></sequenceFlow>
    <serviceTask id="mailtask2" name="Notifica mancata Approvazione DG" activiti:type="mail">
      <extensionElements>
        <activiti:field name="subject">
          <activiti:string>Il DG non ha approvato la tua proposta di determinazione</activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${emailInitiator}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>responsabile@sardegnaricerche.it</activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:string>Il Direttore non ha approvato la tua proposta.</activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression>Il DG non ha approvato la tua proposta di determinazione di ${detSubject}.
Il motivo è il seguente: ${motivazioneDG}.</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="mailtask3" name="Notifica determinazione firmata e datata al proponente" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${emailInitiator}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string>responsabile@sardegnaricerche.it</activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string>Determinazione firmata dal DG</activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:expression>La determinazione n. ${numero_determinazione} del ${data_determinazione} di ${detSubject} è stata approvata e firmata dal DG.</activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>La determinazione n. ${numero_determinazione} del ${data_determinazione} di ${detSubject} è stata approvata e firmata dal DG.</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <parallelGateway id="parallelgateway3" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow28" sourceRef="parallelgateway3" targetRef="mailtask3"></sequenceFlow>
    <serviceTask id="servicetask5" name="Copia allegati a processo chiamante" activiti:class="org.sr.activiti.explorer.delegates.CopyAttachmentsToParent"></serviceTask>
    <sequenceFlow id="flow31" sourceRef="usertask5" targetRef="usertask6"></sequenceFlow>
    <userTask id="usertask6" name="AGI - Inserimento determinazione firmata e archiviazione" activiti:assignee="smaxia">
      <documentation>Allegare la scansione con le firme della determinazione di ${detSubject}.</documentation>
      <extensionElements>
        <activiti:formProperty id="ro_data_determinazione" name="Data Determinazione" type="date" variable="data_determinazione" writable="false"></activiti:formProperty>
        <activiti:formProperty id="ro_numero_determinazione" name="Numero Determinazione" type="textmessage" default="${numero_determinazione}" writable="false"></activiti:formProperty>
        <activiti:taskListener event="complete" class="org.sr.activiti.taskListener.CmisArchiveTaskCompleteListener">
          <activiti:field name="parentFolder">
            <activiti:expression>${idParentProcess}</activiti:expression>
          </activiti:field>
          <activiti:field name="suffix">
            <activiti:expression>determinazione_${numero_determinazione}_${dateFormatUtil.formatSimpleDate(data_determinazione)}</activiti:expression>
          </activiti:field>
          <activiti:field name="documentCategory">
            <activiti:string>determinazione</activiti:string>
          </activiti:field>
          <activiti:field name="minAttachmentsNum">
            <activiti:string>1</activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow37" name="Approvato" sourceRef="exclusivegateway5" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${firmatoDG=='true'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow41" sourceRef="servicetask5" targetRef="endevent2"></sequenceFlow>
    <sequenceFlow id="flow42" sourceRef="startevent1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow43" sourceRef="parallelgateway3" targetRef="exclusivegateway4"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway6" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow44" sourceRef="usertask6" targetRef="exclusivegateway6"></sequenceFlow>
    <sequenceFlow id="flow45" sourceRef="exclusivegateway6" targetRef="parallelgateway3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isAttached}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow46" sourceRef="exclusivegateway6" targetRef="usertask6">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isAttached}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask6" name="Creazione determinazione" activiti:class="org.sr.activiti.explorer.delegates.docGenerate">
      <extensionElements>
        <activiti:field name="data_determinazione">
          <activiti:expression>${data_determinazione}</activiti:expression>
        </activiti:field>
        <activiti:field name="numero_determinazione">
          <activiti:expression>${numero_determinazione}</activiti:expression>
        </activiti:field>
        <activiti:field name="oggetto_determinazione">
          <activiti:expression>${detSubject}</activiti:expression>
        </activiti:field>
        <activiti:field name="nome_rup">
          <activiti:expression>${rup}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow47" sourceRef="usertask3" targetRef="servicetask6"></sequenceFlow>
    <sequenceFlow id="flow48" sourceRef="servicetask6" targetRef="usertask6"></sequenceFlow>
  </process>
